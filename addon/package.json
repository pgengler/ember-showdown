{
  "name": "ember-showdown",
  "version": "2.0.0-pre.1",
  "description": "Ember component to render markdown into HTML.",
  "keywords": [
    "ember-addon",
    "showdown",
    "markdown",
    "component",
    "emberjs"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/pgengler/ember-showdown",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.8",
    "broccoli-source": "^3.0.1",
    "broccoli-string-replace": "^0.1.2",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.0",
    "resolve": "^1.8.1",
    "semver": "^7.3.5",
    "showdown": "^1.8.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ember-template-lint": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.4.7",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "https://pgengler.github.io/ember-showdown/"
  }
}